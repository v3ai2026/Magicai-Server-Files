{
  "name": "magicai-server-files",
  "version": "1.0.0",
  "description": "MagicAI server files with organized structure for AI-powered applications",
  "main": "server/index.js",
  "scripts": {
    "start": "node server/index.js",
    "dev": "nodemon server/index.js",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "migrate": "knex migrate:latest --knexfile config/database.js",
    "migrate:rollback": "knex migrate:rollback --knexfile config/database.js",
    "migrate:make": "knex migrate:make --knexfile config/database.js",
    "seed": "knex seed:run --knexfile config/database.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"**/*.{js,json,md}\""
  },
  "keywords": [
    "magicai",
    "ai",
    "server",
    "api",
    "nodejs",
    "express"
  ],
  "author": "MagicAI Team",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "dotenv": "^16.3.1",
    "cors": "^2.8.5",
    "helmet": "^7.0.0",
    "bcrypt": "^5.1.1",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.2",
    "knex": "^2.5.1",
    "pg": "^8.11.3",
    "express-rate-limit": "^6.10.0",
    "express-validator": "^7.0.1",
    "morgan": "^1.10.0",
    "winston": "^3.10.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "jest": "^29.6.4",
    "eslint": "^8.48.0",
    "prettier": "^3.0.3",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=6.0.0"
  }
}
